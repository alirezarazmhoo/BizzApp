@model BizApp.Areas.Profile.Models.UserPhotosWithProfileDetailViewModel
@{ int count = 1; }
@foreach (var photo in Model.UserPhotos)
{
	<div class="profile-content-all-photos__modal modal fade" id="click-photo-@(count++)" tabindex="@count" role="dialog" aria-labelledby="ModalLabel" aria-hidden="true">
		<div class="modal-dialog" role="document">
			<div class="modal-content">
				<div class="modal-body">
					<div class="profile-content-all-photos__modal__close" data-dismiss="modal">
						<span>بستن</span>
						<svg id="24x24_close" height="24" viewBox="0 0 24 24" width="24"><path d="M17.657 19.07L12 13.415 6.343 19.07 4.93 17.658 10.585 12 4.93 6.343 6.342 4.93 12 10.585l5.657-5.657L19.07 6.34 13.416 12l5.657 5.657-1.413 1.414z"></path></svg>
					</div>
					<div class="row" data-space="10">
						<div class="col-lg-10 col-md-9 col-sm-8">
							<div class="swiper-container">

								<div class="swiper-wrapper">
									<div class="swiper-slide">
										<div class="profile-content-all-photos__modal__image">
											<a href="#">
												<img src="@photo.Path" alt="">
											</a>
										</div>
										<div class="profile-content-all-photos__modal__footer-slide">
											<div class="profile-content-all-photos__modal__footer-slide__right">
												<a href="#">
													<svg id="18x18_grid" height="18" viewBox="0 0 18 18" width="18"><path d="M10 15v-5h5v5h-5zm0-12h5v5h-5V3zm-7 7h5v5H3v-5zm0-7h5v5H3V3z"></path></svg>
													نمایش همه
												</a>
											</div>
											<div class="profile-content-all-photos__modal__footer-slide__left">
												<a href="#">
													<svg id="18x18_share" height="18" viewBox="0 0 18 18" width="18"><path d="M17.714 6.43L13 10.356v-3.03c-1 0-5.097 1.47-6.286 3.62.274-3.08 4.286-5.5 6.286-5.5V2.5l4.714 3.93zM3 4v10h11v-2.5l1-1V15H2V3h8.5l-1 1H3z"></path></svg>
													اشتراک گذاری
												</a>
											</div>
										</div>
									</div>
									<div class="swiper-slide">

										<div class="profile-content-all-photos__modal__image">
											<a href="#">
												<img src="@photo.Path" alt="">
											</a>
										</div>
										@*<div class="profile-content-all-photos__modal__footer-slide">
												<div class="profile-content-all-photos__modal__footer-slide__right">
													<a href="#">
														<svg id="18x18_grid" height="18" viewBox="0 0 18 18" width="18"><path d="M10 15v-5h5v5h-5zm0-12h5v5h-5V3zm-7 7h5v5H3v-5zm0-7h5v5H3V3z"></path></svg>
														نمایش همه
													</a>
												</div>
												<div class="profile-content-all-photos__modal__footer-slide__left">
													<a href="#">
														<svg id="18x18_share" height="18" viewBox="0 0 18 18" width="18"><path d="M17.714 6.43L13 10.356v-3.03c-1 0-5.097 1.47-6.286 3.62.274-3.08 4.286-5.5 6.286-5.5V2.5l4.714 3.93zM3 4v10h11v-2.5l1-1V15H2V3h8.5l-1 1H3z"></path></svg>
														اشتراک گذاری
													</a>
												</div>
											</div>*@
									</div>
								</div>
								<div class="swiper-pagination"></div>
								<div class="swiper-button-next"></div>
								<div class="swiper-button-prev"></div>
							</div>
						</div>
						<div class="col-lg-2 col-md-3 col-sm-4">
							<div class="profile-content-all-photos__modal__content">

								@await Html.PartialAsync("Partials/_UserInfoPartial", Model.ProfileDetail)

								<div class="profile-content-all-photos__modal__content__desc">
									<p>بهترین خدمات هتل حرفه ای Maitres و BARMEN در خانه در پاریس و ایل دو فرانسه</p>
								</div>
								<div class="profile-content-all-photos__modal__content__date">
									<span>6 مارس 2021</span>
								</div>
							</div>
						</div>

					</div>
				</div>

			</div>
		</div>
	</div>
}